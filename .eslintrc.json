{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "airbnb",
        "airbnb/hooks"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "baseui"
    ],
    "rules": {
        "baseui/deprecated-theme-api": "warn",
        "baseui/deprecated-component-api": "warn",
        "baseui/no-deep-imports": "warn"
    }
}
